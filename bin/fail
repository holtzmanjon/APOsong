#!/usr/bin/env python
from astropy.time import Time
import os
import subprocess
import robotic

robotic.aposong.init()
mjd=int(Time.now().mjd)
if os.path.exists('/home/song/APOsong/{:d}'.format(mjd)) :
    robotic.mail('failed!','{:d} exists!'.format(mjd))
else : 
    robotic.mail('suceeded!','{:d} exists!'.format(mjd))
